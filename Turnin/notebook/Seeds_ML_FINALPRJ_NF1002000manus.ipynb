    "    f1_per_class = f1_score(y_test_adj, y_pred, average=None, zero_division=0)\n",
    "\n",
    "    # Display results\n",
    "    print(f\"\\nOverall Metrics:\")\n",
    "    print(f\"  Accuracy:  {accuracy:.4f}\")\n",
